        "typescript": "3.7.4",
        "webpack": "4.41.4",
        "webpack-cli": "3.3.10",
        "webpack-dev-server": "3.10.1",
        "webpack-hot-middleware": "2.25.0"
    },
    "scripts": {
        "preinstall": "node ./scripts/netlify-preinstall.js",
        "dev": "webpack-dev-server --open --colors --hot --mode development --history-api-fallback",
        "build": "webpack --mode production",
        "build-dev": "webpack --mode development",
        "start": "npx serve dist",
        "test": "jest",
        "update": "yarn upgrade-interactive --latest",
        "lint:ts": "eslint ./src/**/*.ts ./src/**/*.tsx  --cache",
        "lint:js": "eslint ./*.js --cache",
        "lint:json": "prettier '*.json'",
        "lint:ci": "circleci config validate",
        "lint:style": "stylelint './src/**/*.tsx' --syntax css-in-js",
        "lint:fix:ts": "yarn lint:ts --fix",
        "lint:fix:js": "yarn lint:js --fix",
        "lint:fix:json": "yarn lint:json --write",
        "lint:fix:style": "yarn lint:style --fix",
        "lint": "run-p -l -n -c --aggregate-output lint:*",
        "lint:fix": "run-p -l -n -c --aggregate-output lint:fix:*",
        "release": "standard-version",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "./.circleci/config.yml": [
            "yarn lint:ci",
            "git add"
        ],
        "*.{js,jsx}": [
            "yarn lint:fix:js",
            "git add"
        ],
        "*.{ts,tsx}": [
            "yarn lint:fix:ts",
            "yarn lint:style",
            "git add"
        ],
        "*.{json}": [
            "yarn lint:fix:json",
            "git add"
        ]
    }
}
