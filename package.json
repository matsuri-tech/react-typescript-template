        "stylelint-config-recess-order": "^2.0.4",
        "stylelint-config-recommended": "^3.0.0",
        "stylelint-config-styled-components": "^0.1.1",
        "ts-jest": "25.2.1",
        "typescript": "3.8.3",
        "webpack": "4.42.0",
        "webpack-bundle-analyzer": "^3.6.1",
        "webpack-cli": "3.3.11",
        "webpack-dev-server": "3.10.3",
        "webpack-hot-middleware": "2.25.0"
    },
    "scripts": {
        "preinstall": "node ./scripts/netlify-preinstall.js",
        "dev": "webpack-dev-server --open --colors --hot --mode development --history-api-fallback",
        "dev:https": "webpack-dev-server --open --colors --hot --mode development --history-api-fallback --https",
        "dev:local": "NODE_ENV=local webpack-dev-server --open --colors --hot --mode development --history-api-fallback",
        "build": "webpack --mode production",
        "build:dev": "webpack --mode development",
        "analyze": "yarn build:dev && webpack-bundle-analyzer ./dist/stats.json --port 8765",
        "start": "npx serve dist",
        "test": "jest",
        "update": "yarn upgrade-interactive --latest",
        "typecheck": "tsc --project tsconfig.json #",
        "lint:ts": "eslint ./src/**/*.ts ./src/**/*.tsx  --cache",
        "lint:js": "eslint ./*.js --cache",
        "lint:json": "prettier \"*.json\"",
        "lint:ci": "circleci config validate",
        "lint:style": "stylelint \"./src/**/*.tsx\" --syntax css-in-js",
        "lint:fix:ts": "yarn lint:ts --fix",
        "lint:fix:js": "yarn lint:js --fix",
        "lint:fix:json": "yarn lint:json --write",
        "lint:fix:style": "yarn lint:style --fix",
        "lint": "run-p -l -n -c --aggregate-output lint:*",
        "lint:fix": "run-p -l -n -c --aggregate-output lint:fix:*",
        "release": "standard-version",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "lint-staged"
        }
    },
    "browserslist": {
        "production": [
            ">0.2% in JP",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "lint-staged": {
        "./.circleci/config.yml": [
            "yarn lint:ci",
            "git add"
        ],
        "*.{js,jsx}": [
            "yarn lint:fix:js",
            "git add"
        ],
        "*.{ts,tsx}": [
            "yarn typecheck",
            "yarn lint:fix:ts",
            "git add"
        ],
        "*.{json}": [
            "yarn lint:fix:json",
            "git add"
        ]
    }
}
