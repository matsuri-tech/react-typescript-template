        "@babel/preset-env": "7.8.7",
        "@babel/preset-react": "7.8.3",
        "@babel/preset-typescript": "7.8.3",
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "@storybook/addon-actions": "^5.3.17",
        "@storybook/addon-docs": "^5.3.17",
